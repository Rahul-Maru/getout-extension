{
	"name": "miau",
	"description": "Base Level Miau",
	"version": "1.0",
	"manifest_version": 3,
	"icons": {
		"16": "imgs/miau-16.png",
		"32": "imgs/miau-32.png",
		"48": "imgs/miau-48.png",
		"128": "imgs/miau-128.png"
	},
	"action": {
		"default_popup": "hello.html"
	},
	"content_scripts": [
		{
			"matches": [
				"https://xyproblem.info/*",
				"https://reddit.com/*"
			],
			"js": [
				"redirect.js"
			],
			"run_at": "document_start"
		}
	]
}