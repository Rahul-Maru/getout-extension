{
	"name": "Get Out!",
	"description": "get out",
	"version": "1.0",
	"manifest_version": 3,
	"icons": {
		"16": "imgs/blast-16.png",
		"32": "imgs/blast-32.png",
		"48": "imgs/blast-48.png",
		"128": "imgs/blast-128.png"
	},
	"action": {
		"default_popup": "popup.html"
	},
	"content_scripts": [
		{
			"matches": [
				"https://xyproblem.info/*",
				"https://www.xyproblem.info/*",
				"https://www.reddit.com/*",
				"https://reddit.com/*"
			],
			"js": [
				"redirect.js"
			],
			"run_at": "document_start"
		}
	],
	"background": {
		"service_worker": "sw.js"
	},
	"web_accessible_resources": [
		{
			"resources": [
				"index.html",
				"return.js"
			],
			"matches": [
				"<all_urls>"
			]
		}
	]
}